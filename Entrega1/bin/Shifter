E_0x1d8ece0 .event edge, v0x1dca130_0, v0x1dca090_0, v0x1da3230_0;
    .scope S_0x1db3d30;
T_0 ;
    %wait E_0x1d8ece0;
    %load/v 8, v0x1dca130_0, 2;
    %cmpi/u 8, 0, 2;
    %jmp/1 T_0.0, 6;
    %cmpi/u 8, 1, 2;
    %jmp/1 T_0.1, 6;
    %cmpi/u 8, 2, 2;
    %jmp/1 T_0.2, 6;
    %jmp T_0.3;
T_0.0 ;
    %load/v 8, v0x1da3230_0, 32;
    %load/v 40, v0x1dca090_0, 5;
    %ix/get 0, 40, 5;
    %shiftr/i0  8, 32;
    %set/v v0x1dc9ff0_0, 8, 32;
    %jmp T_0.3;
T_0.1 ;
    %load/v 8, v0x1da3230_0, 32;
    %load/v 40, v0x1dca090_0, 5;
    %ix/get 0, 40, 5;
    %shiftr/i0  8, 32;
    %set/v v0x1dc9ff0_0, 8, 32;
    %jmp T_0.3;
T_0.2 ;
    %load/v 8, v0x1da3230_0, 32;
    %load/v 40, v0x1dca090_0, 5;
    %ix/get 0, 40, 5;
    %shiftl/i0  8, 32;
    %set/v v0x1dc9ff0_0, 8, 32;
    %jmp T_0.3;
T_0.3 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x1db3c40;
T_1 ;
    %vpi_call 2 17 "$dumpfile", "vcd/Shifter.vcd";
    %vpi_call 2 18 "$dumpvars";
    %movi 8, 1, 32;
    %set/v v0x1dca1e0_0, 8, 32;
    %set/v v0x1dca410_0, 0, 2;
    %movi 8, 1, 5;
    %set/v v0x1dca360_0, 8, 5;
    %delay 10, 0;
    %vpi_call 2 23 "$display", "%b", v0x1dca2b0_0;
    %movi 8, 1, 32;
    %set/v v0x1dca1e0_0, 8, 32;
    %set/v v0x1dca410_0, 0, 2;
    %movi 8, 2, 5;
    %set/v v0x1dca360_0, 8, 5;
    %delay 10, 0;
    %vpi_call 2 28 "$display", "%b", v0x1dca2b0_0;
    %movi 8, 1, 32;
    %set/v v0x1dca1e0_0, 8, 32;
    %set/v v0x1dca410_0, 0, 2;
    %movi 8, 3, 5;
    %set/v v0x1dca360_0, 8, 5;
    %delay 10, 0;
    %vpi_call 2 33 "$display", "%b", v0x1dca2b0_0;
    %movi 8, 1, 32;
    %set/v v0x1dca1e0_0, 8, 32;
    %movi 8, 1, 2;
    %set/v v0x1dca410_0, 8, 2;
    %movi 8, 1, 5;
    %set/v v0x1dca360_0, 8, 5;
    %delay 10, 0;
    %vpi_call 2 38 "$display", "%b", v0x1dca2b0_0;
    %movi 8, 1, 32;
    %set/v v0x1dca1e0_0, 8, 32;
    %movi 8, 1, 2;
    %set/v v0x1dca410_0, 8, 2;
    %movi 8, 2, 5;
    %set/v v0x1dca360_0, 8, 5;
    %delay 10, 0;
    %vpi_call 2 43 "$display", "%b", v0x1dca2b0_0;
    %movi 8, 1, 32;
    %set/v v0x1dca1e0_0, 8, 32;
    %movi 8, 1, 2;
    %set/v v0x1dca410_0, 8, 2;
    %movi 8, 3, 5;
    %set/v v0x1dca360_0, 8, 5;
    %delay 10, 0;
    %vpi_call 2 48 "$display", "%b", v0x1dca2b0_0;
    %movi 8, 1, 32;
    %set/v v0x1dca1e0_0, 8, 32;
    %movi 8, 2, 2;
    %set/v v0x1dca410_0, 8, 2;
    %movi 8, 1, 5;
    %set/v v0x1dca360_0, 8, 5;
    %delay 10, 0;
