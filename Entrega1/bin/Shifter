S_0x1db3c40 .scope module, "Shifter_tb" "Shifter_tb" 2 1;
 .timescale 0 0;
v0x1dca1e0_0 .var "in", 31 0;
v0x1dca2b0_0 .net "result", 31 0, v0x1dc9ff0_0; 1 drivers
