#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0xde3500 .scope module, "Shifter_tb" "Shifter_tb" 2 1;
 .timescale 0 0;
v0xdf6180_0 .var "in", 31 0;
v0xdf6250_0 .net "result", 31 0, v0xdf5f90_0; 1 drivers
v0xdf6300_0 .var "shiftamt", 4 0;
v0xdf63b0_0 .var "shiftop", 1 0;
S_0xde35f0 .scope module, "teste" "Shifter" 2 13, 3 1, S_0xde3500;
 .timescale 0 0;
v0xdcf360_0 .net "in", 31 0, v0xdf6180_0; 1 drivers
v0xdf5f90_0 .var "result", 31 0;
v0xdf6030_0 .net "shiftamt", 4 0, v0xdf6300_0; 1 drivers
v0xdf60d0_0 .net "shiftop", 1 0, v0xdf63b0_0; 1 drivers
E_0xde4aa0 .event edge, v0xdf60d0_0, v0xdf6030_0, v0xdcf360_0;
    .scope S_0xde35f0;
T_0 ;
    %wait E_0xde4aa0;
    %load/v 8, v0xdf60d0_0, 2;
    %cmpi/u 8, 0, 2;
    %jmp/1 T_0.0, 6;
    %cmpi/u 8, 1, 2;
    %jmp/1 T_0.1, 6;
    %cmpi/u 8, 2, 2;
    %jmp/1 T_0.2, 6;
    %jmp T_0.3;
T_0.0 ;
    %load/v 8, v0xdcf360_0, 32;
    %load/v 40, v0xdf6030_0, 5;
    %ix/get 0, 40, 5;
    %shiftr/i0  8, 32;
    %set/v v0xdf5f90_0, 8, 32;
    %jmp T_0.3;
T_0.1 ;
    %load/v 8, v0xdcf360_0, 32;
    %load/v 40, v0xdf6030_0, 5;
    %ix/get 0, 40, 5;
    %shiftr/i0  8, 32;
    %set/v v0xdf5f90_0, 8, 32;
    %jmp T_0.3;
T_0.2 ;
    %load/v 8, v0xdcf360_0, 32;
    %load/v 40, v0xdf6030_0, 5;
    %ix/get 0, 40, 5;
    %shiftl/i0  8, 32;
    %set/v v0xdf5f90_0, 8, 32;
    %jmp T_0.3;
T_0.3 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0xde3500;
T_1 ;
    %vpi_call 2 17 "$dumpfile", "Shifter.vcd";
    %vpi_call 2 18 "$dumpvars";
    %movi 8, 1, 32;
    %set/v v0xdf6180_0, 8, 32;
    %set/v v0xdf63b0_0, 0, 2;
    %movi 8, 1, 5;
    %set/v v0xdf6300_0, 8, 5;
    %delay 10, 0;
    %vpi_call 2 23 "$display", "%b", v0xdf6250_0;
    %movi 8, 1, 32;
    %set/v v0xdf6180_0, 8, 32;
    %set/v v0xdf63b0_0, 0, 2;
    %movi 8, 2, 5;
    %set/v v0xdf6300_0, 8, 5;
    %delay 10, 0;
    %vpi_call 2 28 "$display", "%b", v0xdf6250_0;
    %movi 8, 1, 32;
    %set/v v0xdf6180_0, 8, 32;
    %set/v v0xdf63b0_0, 0, 2;
    %movi 8, 3, 5;
    %set/v v0xdf6300_0, 8, 5;
    %delay 10, 0;
    %vpi_call 2 33 "$display", "%b", v0xdf6250_0;
    %movi 8, 1, 32;
    %set/v v0xdf6180_0, 8, 32;
    %movi 8, 1, 2;
    %set/v v0xdf63b0_0, 8, 2;
    %movi 8, 1, 5;
    %set/v v0xdf6300_0, 8, 5;
    %delay 10, 0;
    %vpi_call 2 38 "$display", "%b", v0xdf6250_0;
    %movi 8, 1, 32;
    %set/v v0xdf6180_0, 8, 32;
    %movi 8, 1, 2;
    %set/v v0xdf63b0_0, 8, 2;
    %movi 8, 2, 5;
    %set/v v0xdf6300_0, 8, 5;
    %delay 10, 0;
    %vpi_call 2 43 "$display", "%b", v0xdf6250_0;
    %movi 8, 1, 32;
    %set/v v0xdf6180_0, 8, 32;
    %movi 8, 1, 2;
    %set/v v0xdf63b0_0, 8, 2;
    %movi 8, 3, 5;
    %set/v v0xdf6300_0, 8, 5;
    %delay 10, 0;
    %vpi_call 2 48 "$display", "%b", v0xdf6250_0;
    %movi 8, 1, 32;
    %set/v v0xdf6180_0, 8, 32;
    %movi 8, 2, 2;
    %set/v v0xdf63b0_0, 8, 2;
    %movi 8, 1, 5;
    %set/v v0xdf6300_0, 8, 5;
    %delay 10, 0;
    %vpi_call 2 53 "$display", "%b", v0xdf6250_0;
    %movi 8, 1, 32;
    %set/v v0xdf6180_0, 8, 32;
    %movi 8, 2, 2;
    %set/v v0xdf63b0_0, 8, 2;
    %movi 8, 2, 5;
    %set/v v0xdf6300_0, 8, 5;
    %delay 10, 0;
    %vpi_call 2 58 "$display", "%b", v0xdf6250_0;
    %movi 8, 1, 32;
    %set/v v0xdf6180_0, 8, 32;
    %movi 8, 2, 2;
    %set/v v0xdf63b0_0, 8, 2;
    %movi 8, 3, 5;
    %set/v v0xdf6300_0, 8, 5;
    %delay 10, 0;
    %vpi_call 2 63 "$display", "%b", v0xdf6250_0;
    %vpi_call 2 64 "$finish";
    %end;
    .thread T_1;
